{
  "database": "Zotero",
  "noteImportFolder": "Lab/Zotero",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "jpg",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "Bibliography",
      "format": "formatted-bibliography",
      "cslStyle": "apa"
    }
  ],
  "exportFormats": [
    {
      "name": "Import #1",
      "outputPathTemplate": "Lab/Zotero/Paper/{{citekey}}.md",
      "imageOutputPathTemplate": "Lab/Zotero/Image/{{citekey}}/",
      "imageBaseNameTemplate": "image",
      "templatePath": "Lab/Zotero/Template.md"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": true,
  "whichNotesToOpenAfterImport": "first-imported-note",
  "shouldConcat": true
}